[core]
  editor = vim

[alias]
  co   = checkout
  br   = branch
  cmt  = commit -m
  st   = status
  pu   = push
  pl   = pull
  adog = log --all --decorate --oneline --graph --color --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit

[pull]
  rebase = true
